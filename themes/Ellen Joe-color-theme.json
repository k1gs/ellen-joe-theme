{
	"name": "Ellen Joe",
	"type": "dark",
	"colors": {
		"editor.background": "#121214",
		"editor.foreground": "#e0e0e0",
		"editorCursor.foreground": "#ff3399",
		"editor.lineHighlightBackground": "#ffffff0a",
		"editor.selectionBackground": "#ff334440",
		"editorLineNumber.foreground": "#5c5c5c",
		"editorLineNumber.activeForeground": "#ffffff",
		"sideBar.background": "#0f0f11",
		"sideBar.foreground": "#cccccc",
		"sideBarSectionHeader.background": "#1a1a1d",
		"editorGroupHeader.tabsBackground": "#0f0f11",
		"tab.activeBackground": "#1a1a1d",
		"tab.activeForeground": "#ff3392",
		"tab.border": "#1a1a1d",
		"tab.inactiveBackground": "#0f0f11",
		"tab.inactiveForeground": "#666666",
		"statusBar.background": "#8a1c41",
		"statusBar.foreground": "#ffffff",
		"titleBar.activeBackground": "#0f0f11",
		"titleBar.activeForeground": "#eeeeee",
		"activityBar.background": "#0f0f11",
		"activityBar.foreground": "#ff33b1",
		"input.background": "#1a1a1d",
		"input.border": "#ff3399",
		"focusBorder": "#ff3399"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6e6e6e"
			}
		},
		{
			"name": "Keywords (Ice Blue)",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control"
			],
			"settings": {
				"foreground": "#80D4FF"
			}
		},
		{
			"name": "Strings (Soft Red/Pink)",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#FF99A5"
			}
		},
		{
			"name": "Functions & Methods (White)",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"support.function"
			],
			"settings": {
				"foreground": "#FFFFFF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Variables & Properties (Silver)",
			"scope": [
				"variable",
				"variable.other.object",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#CCCCCC"
			}
		},
		{
			"name": "Numbers & Constants (Deep Red)",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#FF3344"
			}
		},
		{
			"name": "Classes & Types (Pale Blue)",
			"scope": [
				"entity.name.type",
				"entity.name.class",
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#B3E5FC"
			}
		},
		{
			"name": "Tags (HTML/XML) - Red",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml"
			],
			"settings": {
				"foreground": "#FF3344"
			}
		},
		{
			"name": "Attributes - Ice Blue",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#80D4FF"
			}
		}
	]
}